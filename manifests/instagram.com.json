{
    "celobox_manifest": "0.2.0",
    "throttle": 5,
    "needs_real_browser": true,
    "hosts": ["instagram.com"],
    "login": {
        "url": "https://www.instagram.com/accounts/login/",
        "username": "input[name=\"username\"]",
        "password": "input[name=\"password\"]",
        "submit": "button",
        "success": {
            "landing": "https://www.instagram.com/"
        }
    },
    "password": {
        "url": "https://www.instagram.com/accounts/password/change/",
        "old_password": "#cppOldPassword",
        "new_password": "#cppNewPassword",
        "verify_password": "#cppConfirmPassword",
        "submit": "button"
    },
    "notes": [
        "Can't re-use an old password: Create a new password you haven't used before."
    ]
}
