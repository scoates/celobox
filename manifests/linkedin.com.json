{
    "celobox_manifest": "0.2.0",
    "hosts": ["linkedin.com"],
    "wait_after_login": 5,
    "needs_real_browser": true,
    "login": {
        "url": "https://www.linkedin.com/uas/login",
        "username": "#session_key-login",
        "password": "#session_password-login",
        "submit": "#btn-primary",
        "success": {
            "landing": "https://www.linkedin.com/feed/"
        }
    },
    "password": {
        "url": "https://www.linkedin.com/uas/change-password",
        "old_password": "input[name=\"oldPassword\"]",
        "new_password": "input[name=\"new_password\"]",
        "verify_password": "input[name=\"new_password_again\"]",
        "submit": "#change_password button"
    },
    "notes": []
}
