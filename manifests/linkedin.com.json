{
    "celobox_manifest": "0.2.0",
    "hosts": ["linkedin.com"],
    "wait_after_login": 5,
    "needs_real_browser": true,
    "login": {
        "url": "https://www.linkedin.com/uas/login",
        "username": "#session_key-login",
        "password": "#session_password-login",
        "submit": "#btn-primary",
        "success": {
            "landing": ["https://www.linkedin.com/feed/", "https://www.linkedin.com/feed/?trk=", "https://www.linkedin.com/nhome/"]
        }
    },
    "password": {
        "url": "https://www.linkedin.com/uas/change-password",
        "old_password": "#oldPassword-editPassword",
        "new_password": "#new_password-newPassword-editPassword",
        "verify_password": "#new_password_again-newPassword-editPassword",
        "submit": "#change-password-submit"
    },
    "notes": []
}
