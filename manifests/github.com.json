{
    "celobox_manifest": "0.2.0",
    "hosts": ["github.com"],
    "login": {
        "url": "https://github.com/login",
        "username": "input[name=\"login\"]",
        "password": "input[name=\"password\"]",
        "submit": "input[name=\"commit\"]",
        "success": {
            "landing": "https://github.com/"
        }
    },
    "password": {
        "url": "https://github.com/settings/admin",
        "old_password": "input[name=\"user[old_password]\"]",
        "new_password": "input[name=\"user[password]\"]",
        "verify_password": "input[name=\"user[password_confirmation]\"]",
        "submit": "#change_password button"
    },
    "notes": [
        "Passwords must contain a number."
    ]
}
